{
  "name": "anunnaki-brewing",
  "version": "0.0.1",
  "description": "Application for a future brewery written in node.js and Typescript",
  "author": "Rich Henry <architect.rich.henry@gmail.com>",
  "repository": "https://github.com/roatanrich/anunnaki-brewing",
  "license": "MIT",
  "keywords": [
    "typescript",
    "brewing",
    "node",
    "anunnaki"
  ],
  "main": "./dist/tsc/src/main.js",
  "types": "./dist/tsc/src/main.d.ts",
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "dev": "nodemon src/main.ts",
    "start": "tsc -p tsconfig.json && node dist/tsc/src/main.js",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/main.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build",
    "kill": "rm -rf node_modules",
    "kill-local": "del /s /q node_modules",
    "clean": "rm -rf dist build package"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/node": "^17.0.45",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "27.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "typescript": ">= 3.0.0"
  },
  "dependencies": {
    "@types/morgan": "^1.9.4",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fastify": "^4.13.0",
    "fs": "^0.0.1-security",
    "generate-api-key": "^1.0.2",
    "http-server": "^14.1.1",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.20.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
